        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">

                    <div class="row">
                        <div class="col-md-12">
                            <div class="card-box">

                                <div id="dragTree">
                                    <ul>
                                        <li>
                                            شنبه
                                            <ul>
                                                @foreach (var Date in ViewBag.Date)
                                                {
                                                    if (Date.DateId > 0 && Date.DateId <= 5)
                                                    {
                                                        <li data-jstree='{"selected":false,"type":"file"}'>
                                                            @Date.DateName
                                                            <ul>
                                                                @foreach (var MasterDate in ViewBag.MasterDate)
                                                                {
                                                                    if ((MasterDate.DateId == Date.DateId) && MasterDate.Status != -1)
                                                                    {
                                                                        foreach (var Master in ViewBag.Master)
                                                                        {
                                                                            if (MasterDate.MasterCode == Master.MasterCode)
                                                                            {
                                                                                <li data-jstree='{"opened":true}'>
                                                                                    @Master.MasterName
                                                                                    <ul>
                                                                                        @foreach (var MasterLesson in ViewBag.MasterLesson)
                                                                                        {
                                                                                            foreach (var Lesson in ViewBag.Lesson)
                                                                                            {
                                                                                                if (MasterLesson.MasterCode == Master.MasterCode && MasterLesson.LessonCode == Lesson.LessonCode)
                                                                                                {
                                                                                                    <li data-jstree='{"type":"file"}'>@Lesson.LessonName</li>
                                                                                                }
                                                                                            }
                                                                                        }
                                                                                    </ul>
                                                                                </li>
                                                                            }
                                                                        }
                                                                    }
                                                                }
                                                            </ul>
                                                        </li>
                                                    }
                                                }
                                            </ul>
                                        </li>
                                        <li>
                                            یکشنبه
                                            <ul>
                                                @foreach (var Date in ViewBag.Date)
                                                {
                                                    if (Date.DateId > 5 && Date.DateId <= 10)
                                                    {
                                                <li data-jstree='{"selected":false,"type":"file"}'>
                                                    @Date.DateName
                                                    <ul>
                                                        @foreach (var MasterDate in ViewBag.MasterDate)
                                                                {
                                                                    if (MasterDate.DateId == Date.DateId)
                                                                    {
                                                                        foreach (var Master in ViewBag.Master)
                                                                        {
                                                                            if (MasterDate.MasterCode == Master.MasterCode)
                                                                            {
                                                                <li data-jstree='{"opened":true}'>
                                                                    @Master.MasterName
                                                                    <ul>
                                                                        @foreach (var MasterLesson in ViewBag.MasterLesson)
                                                                                        {
                                                                                            foreach (var Lesson in ViewBag.Lesson)
                                                                                            {
                                                                                                if (MasterLesson.MasterCode == Master.MasterCode && MasterLesson.LessonCode == Lesson.LessonCode)
